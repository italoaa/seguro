{
    "build": {
	"beforeDevCommand": "",
	"beforeBuildCommand": "",
	"devPath": "../src",
	"distDir": "../src",
	"withGlobalTauri": true
    },
    "package": {
	"productName": "seguro",
	"version": "1.0.0"
    },
    "tauri": {
	"allowlist": {
	    "all": false,
	    "shell": {
		"all": false,
		"open": true
	    },
	    "fs" : {
		"exists": true,
		"removeDir" :true,
		"removeFile" :true,
		"scope" : ["$DESKTOP/*"]
	    },
	    "path": {
		"all": true
	    },
	    "dialog" : {
		"message" : true
	    }
	},
	"bundle": {
	    "active": true,
	    "targets": "all",
	    "identifier": "com.tauri.app",
	    "icon": [
		"icons/32x32.png",
		"icons/128x128.png",
		"icons/128x128@2x.png",
		"icons/icon.icns",
		"icons/icon.ico"
	    ]
	},
	"security": {
	    "csp": null
	},
	"windows": [
	    {
		"fullscreen": false,
		"resizable": false,
		"title": "seguro",
		"width": 500,
		"height": 800
	    }
	]
    }
}
